# React Environment Variables Example
# Copy this file to .env and update the values as needed

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=10000

# Authentication
REACT_APP_JWT_SECRET=your-jwt-secret-key
REACT_APP_TOKEN_EXPIRATION=24h

# Payment Integration
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id

# Application Settings
REACT_APP_APP_NAME=Restaurant Ordering System
REACT_APP_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_RESERVATIONS=true
REACT_APP_ENABLE_DELIVERY_TRACKING=true
REACT_APP_ENABLE_REVIEWS=true

# Development Settings
REACT_APP_DEBUG_MODE=true